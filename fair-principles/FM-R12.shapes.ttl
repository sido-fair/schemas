# (Meta)data are richly described with a plurality of accurate and relevant attributes
# (meta)data are associated with detailed provenance

PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX void: <http://rdfs.org/ns/void#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX foaf: <http://xmlns.com/foaf/0.1/>
PREFIX wv: <http://vocab.org/waiver/terms/norms>
PREFIX sd: <http://www.w3.org/ns/sparql-service-description#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX schema: <http://schema.org/>

PREFIX sido: <https://w3id.org/def/sido-fair#>

sido:FAIR_R11_Shape a sh:NodeShape ;
    sh:targetClass void:Dataset ;
    sh:message "R1.1: (Meta)data is voorzien van herkomstinformatie"@nl ;
    sh:message "R1.1: (Meta)data are associated with detailed provenance"@en ;
    sh:property [
        sh:path schema:maintainer ; # :provider, :publisher, :sdPublisher (struct dat), :sourceOrganization
        # dcterms:source, dcterms:publisher ...
        sh:minCount 1 ;
        sh:class schema:Organization ;
    ]
    .
